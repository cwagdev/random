[alias]
	st = status
	ci = commit
	purr = pull --rebase
	glconf = config --global
	df = diff
	l = log --graph --pretty=format:'%C(yellow)%h%C(cyan)%d%Creset %s %C(white)- %an, %ar%Creset'
	alias = !git config --list | grep 'alias\\.' | sed 's/alias\\.\\([^=]*\\)=\\(.*\\)/\\1\\\t => \\2/' | sort
	undo = reset --hard HEAD
[user]
	name = Chris Wagner
	email = cwagner118@gmail.com
[color]
	ui = true
[push]
	default = simple
